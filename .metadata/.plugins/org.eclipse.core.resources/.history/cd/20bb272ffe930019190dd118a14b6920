package com.json;

import java.io.FileWriter;
import java.io.IOException;

public class SetJson {
	public void write(String string,String file) {
		FileWriter writer;
        try {
            writer = new FileWriter("E:/token.txt");
            writer.write(string);
            writer.flush();
            writer.close();
        } catch (IOException e) {
            e.printStackTrace();
        }
	}
}
