package com.json;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;

public class WriteFloder {
	File file222 = new File("E:\\SpringBootCode\\SpringBoot\\test.xml");
	public void main(File file) throws FileNotFoundException {
		//获取目标文件夹
		File destDir = new File("e:/code/java/jad");
		if(!destDir.exists()) {
            destDir.mkdir();
        }
		//
		System.out.println(file.getName());
		String fileString = file.getName();
		FileOutputStream fos = new FileOutputStream(new File(destDir, fileString));
	}
}
